library ieee;
use ieee.std_logic_1164.all;
use ieee.numeric_std.all;

entity counter is
	port (
		-- Active-low reset
		reset: in std_logic;
		-- Active-high enable
		enable: in std_logic;
		-- Input signal
		input: in std_logic;
		-- Count output
		output: out positive;
	);
end entity counter;

architecture counter_arch of counter is
end architecture counter_arch;